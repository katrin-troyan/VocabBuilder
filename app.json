{
  "expo": {
    "name": "VocabBuilder",
    "slug": "VocabBuilder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#85AA9F"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#85AA9F"
      },
      "package": "com.ket.tro.VocabBuilder"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
